{#


Example Vars file
---
INTERFACE:
  GROUP1:
    PORT:
      - "1/3"
      - "1/4"
    DESC: "example access port"
    SHUTDOWN: False
    VLAN: 7
#}

{% for port in item.value.PORT %}
interface {{ port }}
  description {{ item.value.DESC }}
  switchport access vlan {{ item.value.VLAN }}
{% if item.value.SHUTDOWN %}
  shutdown
  {% else %}
  no shutdown
  {% endif %}
exit
{% endfor %}


